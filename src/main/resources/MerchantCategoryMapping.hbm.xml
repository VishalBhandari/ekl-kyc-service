<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.flipkart.logistics.models.MerchantCategoryMapping" table="merchant_category_mapping">
        <meta attribute="class-description">
            This class contains the category detail.
        </meta>
        <id name="id" type="long" column="id">
            <generator class="native"/>
        </id>
        <property name="merchantId" column="merchant_id" type="long"/>

        <many-to-one name="category" column="category_id" unique="false"
                     class="com.flipkart.logistics.models.Category" not-null="true"/>

    </class>
</hibernate-mapping>