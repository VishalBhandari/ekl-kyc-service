<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.flipkart.logistics.models.ServiceRequest" table="service_request">
        <meta attribute="class-description">
            This class contains the list of documents.
        </meta>
        <id name="id" type="long" column="id">
            <generator class="native"/>
        </id>
        <property name="merchantRefId" column="merchant_ref_id" type="long"/>
        <property name="customerJson" column="customer_json" type="string"/>
        <property name="service" column="service" type="string"/>
        <property name="category" column="category" type="string"/>
        <property name="eta" column="eta" type="string"/>
        <property name="docJson" column="doc_json" type="string"></property>
        <property name="requestRefId" column="request_ref_id" type="long"/>

    </class>
</hibernate-mapping>