<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.flipkart.logistics.models.ServiceRequest" table="service_request">
        <meta attribute="class-description">
            This class contains the list of documents.
        </meta>
        <id name="id" type="long" column="id">
            <generator class="native"/>
        </id>
        <property name="merchantReferenceId" column="merchant_ref_id" type="string"/>
        <property name="category" column="category" type="string"/>
        <property name="request" column="request" type="string"/>

    </class>
</hibernate-mapping>